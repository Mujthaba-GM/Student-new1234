        "#explicit type conversions\n",
        "a=10.5\n",
        "print(\"a data type is \",type(a))\n",
        "print(\"a value is\",a)\n",
        "b=int(a)\n",
        "print(\"b data type is\",type(b))\n",
        "print(\"b value is  \",b)\n",
        "#implicit data type conversion\n",
        "c=100\n",
        "d=100.5\n",
        "e=c+d\n",
        "print(\"e value is\",e)\n",
        "print(\"Data type of e is\",type(e))"
