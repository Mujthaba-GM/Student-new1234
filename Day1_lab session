        "a=int(input(\"enter the value of a\"))\n",
        "b=int(input(\"enter the value of b\"))\n",
        "c=int(input(\"Enter the c value\"))\n",
        "if(a>b):\n",
        "  if(a>c):\n",
        "    print(\"a is big\",a)\n",
        "  else:\n",
        "    print(\"C is big,c\")\n",
